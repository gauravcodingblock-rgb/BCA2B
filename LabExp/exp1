package BCAB1;

public class Experment1 {
    public static void main(String[] args) {
        // Program 1: WAP to print a message “Hello JAVA”.
        System.out.println("Hello JAVA");
        int x=18;
        if(x%2==0){
              System.out.println(x+" is Even Number");

        }
        else{
              System.out.println(x+" is Odd Number");

        }

        
    }
    
}
